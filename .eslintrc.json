{
  "import/parser": "babel-eslint",
  "extends": ["airbnb", "plugin:lodash/recommended"],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "linebreak-style": ["warn", "windows"],
    "lodash/import-scope": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "import", "lodash"]
}
